<p class="pageHeader">All Books</p>

<div class="container">
    <table>
        <tr>
            <th>Title</th>
            <th>Author</th>
            <th>Genre</th>
            <th>Loan Status</th>
        </tr>
        #foreach($book in $books )
        <tr>
            <td>$book.getTitle()</td>
            <td>$book.getAuthor()</td>
            <td>$book.getGenre()</td>
            <td>$book.isOnLoan()</td>
        <tr>
            #end
    </table>
</div>